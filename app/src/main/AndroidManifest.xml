<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:dataExtractionRules="@xml/data_extraction_rules"
            android:fullBackupContent="@xml/backup_rules"
            android:icon="@mipmap/ic_launcher"
            android:label="Registro Elettronico"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.App_Registro_Elettronico"
            tools:targetApi="31">


        <activity
                android:name=".MainActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".studenteActivity"/>
        <activity android:name=".docenteActivity"/>
        <activity android:name=".genitoreActivity"/>
        <activity android:name=".alunniDocenteActivity"/>
        <activity android:name=".inserimentoAssenzeActivity"/>
        <activity android:name=".inserimentoNoteActivity"/>
        <activity android:name=".inserimentoValutazioniActivity"/>
        <activity android:name=".notesAdapter"/>
        <activity android:name=".valutazioneAdapter"/>
        <activity android:name=".assenzeAdapter"/>
        <activity android:name=".alunniActivity"/>

    </application>

</manifest>
